<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MatrixApi"
                   namespace="MatrixApi.Domain">

  <class name="User" table="[User]">
    <id name="Id">
      <generator class="identity" />
    </id>
    <property name="Name" />
    <property name="Password" />
    <property name="Email" />

  </class>

</hibernate-mapping>
